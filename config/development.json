{
  "environment": "development",
  "version": "1.0.0",
  "debug": true,
  "openai": {
    "api_key": "sk-your-openai-api-key-here",
    "model": "gpt-4",
    "embedding_model": "text-embedding-ada-002",
    "temperature": 0.1,
    "max_tokens": 2000,
    "timeout": 30,
    "max_retries": 3
  },
  "pinecone": {
    "api_key": "your-pinecone-api-key-here",
    "environment": "us-west1-gcp",
    "index_name": "genai-devops-assistant-dev",
    "dimension": 1536,
    "metric": "cosine",
    "timeout": 30,
    "max_retries": 3
  },
  "redis": {
    "host": "localhost",
    "port": 6379,
    "db": 0,
    "password": null,
    "ssl": false,
    "timeout": 30,
    "max_connections": 10
  },
  "security": {
    "rate_limit_per_minute": 100,
    "max_query_length": 2000,
    "max_history_length": 50,
    "session_timeout": 3600,
    "cors_origins": [
      "http://localhost:8501",
      "http://127.0.0.1:8501",
      "http://localhost:3000"
    ],
    "allowed_hosts": [
      "localhost",
      "127.0.0.1"
    ],
    "api_key_header": "X-API-Key",
    "enable_rate_limiting": true
  },
  "application": {
    "response_timeout": 30,
    "max_context_length": 4000,
    "top_k_retrieval": 5,
    "cache_ttl": 1800,
    "enable_caching": true,
    "enable_monitoring": true,
    "health_check_interval": 30
  },
  "logging": {
    "level": "DEBUG",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file_path": "logs/development.log",
    "max_file_size": 10485760,
    "backup_count": 5,
    "enable_json_logging": false
  }
}