{
  "environment": "testing",
  "version": "1.0.0",
  "debug": false,
  "openai": {
    "api_key": "sk-test-key-for-testing",
    "model": "gpt-3.5-turbo",
    "embedding_model": "text-embedding-ada-002",
    "temperature": 0.0,
    "max_tokens": 1000,
    "timeout": 15,
    "max_retries": 2
  },
  "pinecone": {
    "api_key": "test-pinecone-key",
    "environment": "us-west1-gcp",
    "index_name": "genai-devops-assistant-test",
    "dimension": 1536,
    "metric": "cosine",
    "timeout": 15,
    "max_retries": 2
  },
  "redis": {
    "host": "localhost",
    "port": 6379,
    "db": 1,
    "password": null,
    "ssl": false,
    "timeout": 10,
    "max_connections": 5
  },
  "security": {
    "rate_limit_per_minute": 1000,
    "max_query_length": 1000,
    "max_history_length": 20,
    "session_timeout": 600,
    "cors_origins": [
      "http://localhost:8501",
      "http://127.0.0.1:8501"
    ],
    "allowed_hosts": [
      "localhost",
      "127.0.0.1"
    ],
    "api_key_header": "X-API-Key",
    "enable_rate_limiting": false
  },
  "application": {
    "response_timeout": 15,
    "max_context_length": 2000,
    "top_k_retrieval": 3,
    "cache_ttl": 60,
    "enable_caching": false,
    "enable_monitoring": false,
    "health_check_interval": 60
  },
  "logging": {
    "level": "WARNING",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file_path": null,
    "max_file_size": 5242880,
    "backup_count": 2,
    "enable_json_logging": false
  }
}